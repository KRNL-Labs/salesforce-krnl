<aura:component
    implements="lightning:appHomeTemplate"
    description="KRNL Single column layout"
    access="global"
>
    <aura:attribute name="main" type="Aura.Component[]" access="global" />
    <div aura:id="container">
        <div class="slds-grid">
            <div class="slds-col slds-size_12-of-12 layout-region-main">
                {!v.main}
            </div>
        </div>
    </div>
</aura:component>